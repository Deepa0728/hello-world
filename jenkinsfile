pipeline{
    agent any 
     stages('CI') {
         stage('checkout'){
            steps{
            echo 'Checkout'
        }

        stage('Build'){
            steps{
            //script 
            echo 'checkout'
        }


           }
       }
           stage('Test'){
           steps{
               echo 'Test'
               sh 'mvn clean install'
           }
           
           }   
   }


}

