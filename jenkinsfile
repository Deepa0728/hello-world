pipeline{
   agent any


   stages('CI'){
       
       stage('checkout'){
           steps{
               echo 'checkout '
               checkout scm 
           }
       }

       stage('Build'){
           steps{
               echo 'checkout '
               
           }
       }
           stage('Test'){
           steps{
               echo 'Test'
               sh 'mvn clean install'
           }
           
           }   
   }


}
